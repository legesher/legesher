{
  "name": "legesher",
  "main": "./lib/legesher",
  "version": "0.0.1",
  "description": "a native language translator for code; the bridge to connect the language we speak to the code we write.",
  "activationCommands": {
    "atom-workspace": [
      "legesher:toggle-language",
      "legesher:open-settings"
    ]
  },
  "scripts": {
    "eslint": "node_modules/.bin/eslint /"
  },
  "keywords": [
    "translator",
    "code",
    "language",
    "legesher",
    "bridge",
    "translate",
    "program",
    "open-source"
  ],
  "repository": "https://github.com/legesher/legesher",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "dependencies": {
    "underscore": "^1.9.1"
  },
  "devDependencies": {
    "all-contributors-cli": "^6.9.2",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-prettier": "^3.1.1",
    "prettier": "^1.18.2"
  }
}
