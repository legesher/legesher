{
  "name": "legesher",
  "main": "./lib/legesher",
  "version": "0.0.1",
  "description": "a native language translator for code; the bridge to connect the language we speak to the code we write.",
  "activationCommands": {
    "atom-workspace": [
      "legesher:toggle-language",
      "legesher:open-settings"
    ]
  },
  "scripts": {
    "eslint": "node_modules/.bin/eslint /"
  },
  "keywords": [
    "translator",
    "code",
    "language",
    "legesher",
    "bridge",
    "translate",
    "program",
    "open-source"
  ],
  "repository": "https://github.com/legesher/legesher",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "dependencies": {
    "acorn": "^8.8.1",
    "minimist": ">=1.2.6",
    "underscore": "^1.12.1"
  },
  "devDependencies": {
    "@unibeautify/beautifier-black": "^0.1.2",
    "@unibeautify/beautifier-eslint": "^0.6.0",
    "@unibeautify/beautifier-js-beautify": "^0.4.0",
    "@unibeautify/beautifier-prettier": "^0.9.0",
    "@unibeautify/beautifier-prettydiff": "^0.6.0",
    "@unibeautify/beautifier-yapf": "^0.3.0",
    "@unibeautify/cli": "^0.5.1",
    "all-contributors-cli": "^6.24.0",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-react": "^7.31.11",
    "js-beautify": "^1.14.7",
    "prettier": "^1.19.1",
    "prettydiff2": "^2.2.8",
    "unibeautify-cli": "^0.2.1"
  }
}
